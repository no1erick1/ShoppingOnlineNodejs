    {{# if type}}
<div class="container">
{{# each products }}
<div class="row equal">
{{# each this}}
  <div class="col-sm-6 col-md-4 equal">
    <div class="thumbnail">
      <img src="{{this.imagePath}}" class="img-responsive">
      <div class="caption">
        <h3>{{this.title}}</h3>
             <div class="sumerize2">
                 <p class="description">{{this.description}}</p>
             </div>
      

      <div class="price pull-left" style="margin-left: 0px; margin-bottom: 10px; margin-top: 10px;
        height:33px;
    display: flex;
    justify-content: space-between; 
    width: 330px;">{{this.price}} VND 
    <a href="/delete4/{{this._id}}" class="btn btn-danger pull-right" role="button">Delete</a>
 <a href="/shop/updatepro?id={{this._id}}" class="btn btn-success pull-right" role="button">Edit</a>
          <a href="/details/game-detail/{{this._id}}" class="btn btn-success pull-right" role="button">Details</a>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>
{{/each}}
</div>
{{/if}}
